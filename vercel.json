{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/seed",
            "destination": "/api/seed.js"
        },
        {
            "source": "/api/super-diag",
            "destination": "/api/super-diag.js"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/auth/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/uploads/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}